{
    "manifest_version": 3,
    "name": "Bluesky Post Translator",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["contentScript.js"]
      }
    ],
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  